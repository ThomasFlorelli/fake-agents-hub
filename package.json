{
  "name": "fake-agents-hub",
  "version": "1.0.0",
  "description": "Simple REST API with Fastify, TypeScript and SurrealDB",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test:up": "scripts/bootstrap.sh",
    "test:watch": "vitest watch",
    "test:down": "docker-compose down",
    "test": "npm run test:up && vitest run && npm run test:down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThomasFlorelli/fake-agents-hub.git"
  },
  "author": "ThomasFlorelli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThomasFlorelli/fake-agents-hub/issues"
  },
  "homepage": "https://github.com/ThomasFlorelli/fake-agents-hub#readme",
  "dependencies": {
    "fastify": "^4.28.1",
    "surrealdb": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.16.11",
    "@types/supertest": "^6.0.2",
    "supertest": "^7.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^4.0.6"
  }
}
